{
    "name": "cushystudio",
    "publisher": "rvion",
    "repository": {
        "type": "git",
        "url": "https://github.com/rvion/CushyStudio"
    },
    "description": "ðŸŽ¨ Generative Art Web-IDE and embeddable typescript SDK - based on ComfyUI",
    "private": true,
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**/*.cushy.ts"
    ],
    "main": "./out/main.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "cushystudio.run",
                "displayName": "",
                "selector": [
                    {
                        "filenamePattern": "*.cushy.OLD"
                    }
                ]
            }
        ],
        "configuration": {
            "title": "CushyStudio",
            "properties": {
                "cushystudio.useCodeSnippetsOnMethodSuggest": {
                    "type": "boolean",
                    "default": false,
                    "description": "Complete functions with their parameter signature."
                },
                "cushystudio.tsdk": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "default": null,
                    "description": "Specifies the folder path containing the tsserver and lib*.d.ts files to use."
                }
            }
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "cushy-studio",
                    "title": "Cushy Studio",
                    "icon": "resources/CushyLogo.png"
                }
            ]
        },
        "views": {
            "cushy-studio": [
                {
                    "id": "cushystudio-servers",
                    "name": "Servers"
                },
                {
                    "id": "cushyrun",
                    "name": "Cushy Runs"
                },
                {
                    "id": "cushygraphs",
                    "name": "CushyGraph"
                }
            ]
        },
        "commands": [
            {
                "command": "cushystudio.openwebview",
                "title": "cushystudio.openwebview | Hello World (React + Vite): Show"
            },
            {
                "command": "cushystudio.helloWorld",
                "title": "cushystudio.helloWorld | Hello world"
            },
            {
                "command": "cushystudio.runcurrentscript",
                "title": "cushystudio.runcurrentscript | Run current script"
            },
            {
                "command": "cushystudio.connect",
                "title": "cushystudio.connect | Connect"
            },
            {
                "command": "cushystudio.samplewebview",
                "title": "cushystudio.samplewebview | Open sample webview "
            },
            {
                "command": "cushystudio.openjs",
                "title": "cushystudio.openjs | Open JS"
            }
        ]
    },
    "author": {
        "name": "rvion",
        "url": "https://github.com/rvion/CushyStudio",
        "email": "vion.remi@gmail.com"
    },
    "license": "unlicensed",
    "version": "0.0.64",
    "scripts": {
        "SHELL": "",
            "vscode:dev":        "code --extensionDevelopmentPath=${PWD}",
        "BACK": "",
            "back:dev":        "npm run back:build -- --sourcemap --watch",
            "back:sourcemap":    "npm run back:build -- --sourcemap",
            "back:metafile":     "npm run back:build -- --minify --metafile=out/meta.json",
            "back:build":        "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
        "FRONT": "",
            "web:dev":           "vite",
            "web:build":         "vite build",
            "web:watch":         "vite build --watch",
            "web:preview":       "vite preview",
            "web:inspect":       "npx vite-bundle-visualizer",
        "CHECKS": "",
            "tsc:compile":       "tsc -p ./",
            "tsc:watch":         "tsc -watch -p ./",
            "tsc:check":         "tsc",
        "TESTS": "",
            "pretest":           "npm run compile && npm run lint",
            "lint":              "eslint src --ext ts",
            "test":              "node ./out/test/runTest.js",
        "RELEASE":               "",
            "bump":              "bump patch --commit=v --tag=v --push",
            "vscode:prepublish": "npm run esbuild --minify",
            "logo":              "./scripts/derive-assets.sh",
        "DEV":                   "",
            "js":                "node --loader ts-node/esm --experimental-specifier-resolution=node --experimental-modules",
        "CORE":                  "",
            "sync":              "yarn js src/core/ComfySpec.fetch.ts",
            "lib":               "yarn js src/core/Comfy.gen.ts",
        "COMPILER":              "",
            "old:compile":       "yarn js src/compiler/entry.gen.ts",
            "old:sample":        "yarn js src/compiler/entry.fetch.ts",
            "old:test":          "yarn js src/compiler/test.ts",
            "old:test2":         "yarn js src/compiler/test2.ts",
            "decl":              "yarn js src/sdk/sdkGenerator.ts",
            "decl:list": "tsc -p tsconfig.decl.json --listFiles --noEmit | grep /Users/loco/dev/CushyStudio/src"
    },
    "dependencies": {
        "@rsuite/icons": "1.0.2",
        "@types/vscode-webview": "1.57.1",
        "@types/ws": "8.5.4",
        "@vscode/webview-ui-toolkit": "1.2.2",
        "cytoscape-klay": "3.1.4",
        "cytoscape": "3.23.0",
        "esbuild": "0.17.16",
        "form-data": "4.0.0",
        "mobx-react-lite": "3.4.3",
        "mobx": "6.9.0",
        "nanoid": "4.0.2",
        "node-fetch": "3.3.1",
        "painterro": "1.2.78",
        "pathe": "1.1.0",
        "prettier": "2.8.7",
        "react-dom": "18.2.0",
        "react": "18.2.0",
        "rsuite": "5.31.1",
        "ts-node": "10.9.1",
        "ws": "8.13.0"
    },
    "devDependencies": {
        "@types/cytoscape": "3.19.9",
        "@types/cytoscape-klay": "3.1.0",
        "@types/glob": "8.1.0",
        "@types/mocha": "10.0.1",
        "@types/node": "18.x",
        "@types/prettier": "2.7.2",
        "@types/react": "18.0.35",
        "@types/react-dom": "18.0.11",
        "@types/vscode": "1.77.0",
        "@typescript-eslint/eslint-plugin": "5.58.0",
        "@typescript-eslint/parser": "5.58.0",
        "@vitejs/plugin-react": "3.1.0",
        "@vscode/test-electron": "2.3.0",
        "eslint": "8.38.0",
        "glob": "10.1.0",
        "less": "4.1.3",
        "mocha": "10.2.0",
        "typescript": "5.0.4",
        "vite": "4.2.1"
    }
}
